{
  "connector": {
    "name": "api-key-query-example",
    "version": "1.0.0",
    "description": "Example with API key in query parameter",
    "input": {
      "type": "httpPolling",
      "endpoint": "https://api.example.com/data",
      "schedule": "*/5 * * * *",
      "timeoutMs": 30000,
      "authentication": {
        "type": "api-key",
        "credentials": {
          "key": "${API_KEY}",
          "location": "query",
          "paramName": "apikey"
        }
      }
    },
    "filters": [],
    "output": {
      "type": "httpRequest",
      "endpoint": "https://api.destination.com/data",
      "method": "POST"
    }
  }
}
