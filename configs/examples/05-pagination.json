{
  "connector": {
    "name": "pagination-example",
    "version": "1.0.0",
    "description": "Pagination examples: page-based, offset-based, and cursor-based",
    "input": {
      "type": "httpPolling",
      "endpoint": "https://api.example.com/users",
      "schedule": "*/5 * * * *",
      "timeoutMs": 30000,
      "dataField": "users",
      "pagination": {
        "type": "page",
        "pageParam": "page",
        "totalPagesField": "total_pages"
      },
      "authentication": {
        "type": "bearer",
        "credentials": {
          "token": "${API_TOKEN}"
        }
      }
    },
    "filters": [],
    "output": {
      "type": "httpRequest",
      "endpoint": "https://api.destination.com/users",
      "method": "POST"
    }
  }
}
