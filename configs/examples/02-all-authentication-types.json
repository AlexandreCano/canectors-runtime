{
  "connector": {
    "name": "all-auth-types",
    "version": "1.0.0",
    "description": "Example covering all authentication types: API key, Bearer, Basic, OAuth2",
    "input": {
      "type": "httpPolling",
      "endpoint": "https://api.source.com/data",
      "schedule": "*/5 * * * *",
      "timeoutMs": 30000,
      "authentication": {
        "type": "api-key",
        "credentials": {
          "key": "${SOURCE_API_KEY}",
          "location": "header",
          "headerName": "X-API-Key"
        }
      }
    },
    "filters": [],
    "output": {
      "type": "httpRequest",
      "endpoint": "https://api.destination.com/data",
      "method": "POST",
      "authentication": {
        "type": "bearer",
        "credentials": {
          "token": "${DEST_BEARER_TOKEN}"
        }
      }
    }
  }
}
