{
  "connector": {
    "name": "simple-pipeline",
    "version": "1.0.0",
    "description": "Simple pipeline without authentication",
    "input": {
      "type": "httpPolling",
      "endpoint": "https://api.example.com/data",
      "schedule": "*/5 * * * *",
      "timeoutMs": 30000
    },
    "filters": [],
    "output": {
      "type": "httpRequest",
      "endpoint": "https://api.destination.com/import",
      "method": "POST"
    }
  }
}
